nt(account)\n    },[account]);\n    \n    useEffect(()=>{\n      setmainweb3(web3)\n    },[web3]);\n\n    const connectWallet = async () => {\n        const accounts = await window.ethereum.request({\n            method: \"eth_requestAccounts\",\n        });\n        setAccount(accounts[0]);\n    };\n \n \n\n\n    return (\n      <div>\n        <div className=\"menu\">\n          <div className=\"dropdown-container\">\n            <div>\n              <img src=\"https://opensea.io/static/images/logos/opensea-logo.png\" />\n            </div>\n            <div>\n              <button>\n                <FiChevronDown className=\"down-icon\" />\n              </button>\n            </div>\n          </div>\n          <div className=\"search-container\">\n            <div className=\"icon\">\n              <MdSearch className=\"search-icon\" />\n            </div>\n            <input\n              className=\"search-input\"\n              placeholder=\"Search items, collections, and accounts\"\n            />\n          </div>\n        </div>\n        <div className=\"menu-large\">\n          <div className=\"dropdown-container\">\n            <div>\n              <img src=\"https://opensea.io/static/images/logos/opensea-logo.png\" />\n            </div>\n            <div>\n              <span>\n                <h2><Link to=\"/\" className=\"title\">Opensea</Link></h2>\n              </span>\n            </div>\n          </div>\n          <div className=\"search-container\">\n            <div className=\"icon\">\n              <MdSearch className=\"search-icon\" />\n            </div>\n            <input\n              className=\"search-input\"\n              placeholder=\"Search items, collections, and accounts\"\n            />\n          </div>\n          <div>\n            <ul className=\"menu-item-container\">\n              <li className=\"menu-item\">Browse</li>\n              <li><Link to=\"/activity\" className=\"menu-item\">Activity</Link></li>\n           